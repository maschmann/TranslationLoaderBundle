{
    "name": "asm/translation-loader-bundle",
    "description": "Symfony2 bundle with database translation loader",
    "keywords": ["translation loader", "translation database", "symfony translation"],
    "homepage": "https://github.com/maschmann/TranslationLoaderBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Aschmann",
            "email": "maschmann@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "doctrine/orm": "~2.2,>=2.2.3",
        "symfony/config": "~2.2",
        "symfony/console": "~2.2",
        "symfony/dependency-injection": "~2.2",
        "symfony/filesystem": "~2.2",
        "symfony/finder": "~2.2",
        "symfony/http-kernel": "~2.2",
        "symfony/translation": "~2.2"
    },
    "require-dev": {
        "symfony/framework-bundle": "~2.2",
        "symfony/security-bundle": "~2.2"
    },
    "autoload": {
        "psr-0": {
            "Asm\\TranslationLoaderBundle": ""
        }
    },
    "target-dir": "Asm/TranslationLoaderBundle"
}
